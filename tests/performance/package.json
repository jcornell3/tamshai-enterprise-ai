{
  "name": "@tamshai/performance-tests",
  "version": "1.4.0",
  "description": "Performance tests for Tamshai Enterprise AI using k6",
  "private": true,
  "scripts": {
    "test": "k6 run scenarios/gateway-load.js",
    "test:smoke": "k6 run --vus 1 --duration 10s scenarios/gateway-load.js",
    "test:stress": "k6 run --vus 50 --duration 60s scenarios/gateway-load.js",
    "test:report": "k6 run --out json=results.json scenarios/gateway-load.js"
  },
  "keywords": [
    "tamshai",
    "performance",
    "k6",
    "load-testing"
  ],
